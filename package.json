{
  "name": "@newdash/npm-update-all",
  "description": "update all dependencies for project",
  "version": "0.0.8",
  "bin": {
    "npm-update-all": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newdash/npm-update-all"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf lib",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "build": "tsc",
    "lint": "eslint src --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.17.1",
    "cli-progress": "^3.8.2",
    "colors": "^1.4.0",
    "commander": "^6.2.1",
    "inquirer": "^7.3.3",
    "node-fetch": "^2.6.1",
    "registry-url": "^5.1.0",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@types/cli-progress": "^3.8.0",
    "@types/semver": "^7.3.4",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "commitlint": "^11.0.0",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
